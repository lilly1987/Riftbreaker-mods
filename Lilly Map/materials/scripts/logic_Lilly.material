import * from "materials/scripts/pbr.material"

material logic/tile_light_mask
{ 
	render_group 211
	
	technique
	{
		pass
		{
			depth_check false
			depth_write false

			vertex_program_ref forward_object_texture_vp 
			{
			}

			fragment_program_ref forward_object_texture_fp
			{
			}
			
			texture_unit light_mask 
			{	
				texture materials/textures/error.dds
                filtering bilinear
			}
		}	
	}
}

material logic/caverns_light_mask_lilly_holes_medium : logic/tile_light_mask
{
	set_texture_alias light_mask materials/textures/logic/caverns_light_mask_lilly_holes_medium.dds
}


